{
  "name": "fetch-and-cache",
  "version": "1.0.3",
  "description": "A fetch based library that caches payload requests by using local storage browser API",
  "main": "./dist/bundle.js",
  "scripts": {
    "start": "jest --watch",
    "build": "rollup ./src/index.js --file ./dist/bundle.js --format cjs",
    "test": "jest",
	"lint": "eslint ./src/**/*.js ./tests/**/*.js",
	"fixlint": "eslint ./src/**/*.js ./tests/**/*.js --fix",
    "prepare": "npm run lint && npm run test && npm run build"
  },
  "author": "Vagner F Santos",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "babel-jest": "^24.3.1",
    "eslint": "^5.15.1",
    "eslint-config-google": "^0.12.0",
    "jest": "^24.3.1",
    "rollup": "^1.6.0"
  }
}
